 //задача 3//
 cout << "Введите длину ребра куба: ";
 double a;
 cin >> a;
 double b;
 double c;
 b = 4 * a * a;
 c = a * a * a;
 cout << "\nПлощадь боковой поверхности:" << b << endl;
 cout << "\nОбъем: " << c << endl;

 //задача 14//
 double m, m1, r, F;
 cout << "\nВведите массу тел: ";
 cin >> m >> m1;
 cout << "\nВведите расстояние между телами: ";
 cin >> r;
 F = 6.67 * pow(10, -11) * m * m1 / (r * r);
 cout << "F= " << F << endl;

 //задача 17//
 double S, R;
 cout << "Введите радиус внешней окружности: ";
 cin >> R;
 if (R <= 20) {
  cout << "\nВнешний радиус должен быть больше: " << r << endl;
 }

 S = 3.14 * ((R * R) - (20 * 20));
 cout << "\nПлощадь кольца: " << S << endl;

 //задача 19//
 double t, S1, S2, rs, sk;
 cout << "Введите начальные скорости двух тел(м/мин): ";
 cin >> S1 >> S2;
 cout << "\nВведите значение ускорения(м/мин): ";
 cin >> sk;
 cout << "\nВведите расстояние между двумя телами(м): ";
 cin >> rs;
 t = rs - ((S1 + sk) + (S2 + sk));
 cout << "\nВремя через которое встретятся два тела(мин): " << t << endl;

 //задача 38//
 double x, y, z;
 cout << "Введите значение x: ";
 cin >> x;
 cout << "\nВведите значение y: ";
 cin >> y;

 if (x > y) {
  z = x - y;
 }
 else {
  z = y - x + 1;
 }
 cout << "\nz= " << z << endl;

  //ЗАДАЧА 41//
 double d, i, g; 
 cout << "введите значение d" << endl;
 cin >> d;
 cout << "введите значение i" << endl;
 cin >> i;
 cout << "введите значение g" << endl;
 cin >> g;

 if (d, i, g < 4) {
  cout << "эти числа принадлежащие интервалу (1..3)" << endl;
 }
 else {
  cout << "Ошибка.введите значения принадлежащие интервалу(1..3)";
